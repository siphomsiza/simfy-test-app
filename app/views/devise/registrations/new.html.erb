<div class="panel panel-default">
	<div class="panel-heading">
		<strong class="panel-title">Sign up</strong>
		<div class="clearfix "></div>
	</div>
	<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
	<div class="panel-body ">
		<%= render 'shared/form_errors', :f => f %>
		<div class="form-group">
			<label for="inputEmail3" class="col-sm-2 control-label">First name</label>
			<div class="col-sm-4">
				<%= f.text_field :first_name, :autofocus => false,:class=>"form-control",:placeholder=>"First Name"%>
			</div>
		</div>
		<div class="clearfix"></div>
		<div class="clearfix"></div>
		<div class="form-group">
			<br/>
			<label for="inputEmail3" class="col-sm-2 control-label">Last Name</label>
			<div class="col-sm-4">
				<%= f.text_field :last_name, :autofocus => false ,:class=>"form-control",:placeholder=>"Last name"%>
			</div>
		</div>
		<div class="clearfix"></div>
		<div class="clearfix"></div>
		<div class="form-group">
			<br/>
			<label for="inputEmail3" class="col-sm-2 control-label">Email</label>
			<div class="col-sm-4">
				<%= f.email_field :email, :autofocus => true ,:class=>"form-control",:placeholder=>"Email"%>
			</div>
		</div>
		<div class="clearfix"></div>
		<div class="clearfix"></div>
		<div class="form-group">
			<br/>
			<label for="inputPassword3" class="col-sm-2 control-label">Password</label>
			<div class="col-sm-4">
				<%= f.password_field :password ,:class=>"form-control" ,:id=>"inputPassword3" ,:placeholder=>"Password"%>
			</div>
		</div>
		<div class="clearfix"></div>
		<div class="clearfix"></div>
		<div class="form-group">
			<br/>
			<label for="inputPassword3" class="col-sm-2 control-label">Password confirm</label>
			<div class="col-sm-4">
				<%= f.password_field :password_confirmation ,:class=>"form-control" ,:id=>"inputPassword3" ,:placeholder=>"Password confirmation"%>
			</div>
		</div>
		<div class="clearfix"></div>
		<br/>
		<%= render "devise/shared/links" %>
	</div>
	<div class="form-actions panel-footer" >
		<span class="pull-right"> <%= html_submit "Sign up" %> </span>
		<div class="clearfix"></div>
	</div>
	<%end%>
</div>
